{
  "name": "Voice Browsing",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is an extension for voice only browsing.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
        "default_icon": "icons/icon38.png"
    },
  "permissions": ["<all_urls>"]
}
